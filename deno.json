{
  "nodeModulesDir": "auto",
  "name": "coresvc",
  "version": "0.1.0",
  "exports": {
    ".": "./main.ts"
  },
  "tasks": {
    "check": "deno fmt --check . && deno lint . && deno check",
    "cache": "deno cache main.ts",
    "dev": "vite",
    "build": "vite build",
    "start": "deno serve -A _fresh/server.js",
    "update-2": "deno run -Ar jsr:@fresh/update@latest",
    "db-push": "deno task db-push-dev && deno task db-push-prod",
    "db-push-dev": "drizzle-kit --config .config/drizzle-dev.config.ts push",
    "db-push-prod": "drizzle-kit --config .config/drizzle-prod.config.ts push",
    "db-studio-dev": "drizzle-kit --config .config/drizzle-dev.config.ts studio",
    "db-studio-prod": "drizzle-kit --config .config/drizzle-prod.config.ts studio",
    "playground-ddns": "deno run -A --unstable-kv --env-file --watch playground/ddns_playground.local.ts"
  },
  "lint": {
    "rules": {
      "tags": [
        "fresh",
        "recommended"
      ]
    }
  },
  "deploy": {
    "project": "coresvc",
    "entrypoint": "./main.ts"
  },
  "exclude": [
    "**/_fresh/*"
  ],
  "imports": {
    "@/": "./",
    "@contracts/": "./contracts/",
    "@fresh/plugin-tailwind": "jsr:@fresh/plugin-tailwind@0.0.1-alpha.9",
    "@awesome.me/webawesome": "npm:@awesome.me/webawesome@^3.0.0-beta.6",
    "@fresh/plugin-vite": "jsr:@fresh/plugin-vite@0.10.2",
    "@iconify-json/mdi": "npm:@iconify-json/mdi@^1.2.3",
    "@iconify-json/mdi-light": "npm:@iconify-json/mdi-light@^1.2.2",
    "@iconify-json/mynaui": "npm:@iconify-json/mynaui@^1.2.16",
    "@iconify/tailwind4": "npm:@iconify/tailwind4@^1.0.6",
    "@libsql/client": "npm:@libsql/client@^0.15.15",
    "@opentelemetry/api": "npm:@opentelemetry/api@^1.9.0",
    "@std/dotenv": "jsr:@std/dotenv@^0.225.5",
    "@std/encoding": "jsr:@std/encoding@^1.0.10",
    "@ts-rest/core": "npm:@ts-rest/core@3.53.0-rc.1",
    "@valibot/valibot": "jsr:@valibot/valibot@^1.1.0",
    "date-fns": "npm:date-fns@^4.1.0",
    "@tailwindcss/vite": "npm:@tailwindcss/vite@^4.1.15",
    "cloudflare": "npm:cloudflare@^4.5.0",
    "drizzle-kit": "npm:drizzle-kit@^0.31.5",
    "drizzle-orm": "npm:drizzle-orm@^0.44.6",
    "neverthrow": "npm:neverthrow@^8.2.0",
    "p-map": "npm:p-map@^7.0.3",
    "preact": "npm:preact@^10.27.2",
    "@preact/signals": "npm:@preact/signals@^2.3.2",
    "tailwind-merge": "npm:tailwind-merge@^3.3.1",
    "tailwindcss": "npm:tailwindcss@4.1.11",
    "tough-cookie": "npm:tough-cookie@^5.1.2",
    "vite": "npm:vite@^7.1.11",
    "zod": "npm:zod@^3.25.76",
    "fresh": "jsr:@fresh/core@^2.1.2"
  },
  "compilerOptions": {
    "jsx": "precompile",
    "jsxImportSource": "preact",
    "lib": [
      "dom",
      "dom.asynciterable",
      "dom.iterable",
      "deno.ns"
    ],
    "jsxPrecompileSkipElements": [
      "a",
      "img",
      "source",
      "body",
      "html",
      "head",
      "title",
      "meta",
      "script",
      "link",
      "style",
      "base",
      "noscript",
      "template"
    ],
    "types": [
      "vite/client"
    ]
  },
  "unstable": [
    "kv"
  ]
}